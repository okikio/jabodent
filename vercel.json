{
    "routes": [
        {
            "src": "/css/(.*)",
            "headers": {
                "Cache-Control": "public, max-age=36000",
                "Content-Type": "text/css"
            },
            "dest": "/css/$1"
        },
        {
            "src": "/js/(.*)",
            "headers": {
                "Cache-Control": "public, max-age=36000",
                "Content-Type": "text/javascript"
            },
            "dest": "/js/$1"
        },
        {
            "src": "/js/theme.min.js",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable",
                "Content-Type": "text/javascript"
            },
            "dest": "/js/theme.min.js"
        },
        {
            "src": "/*.json",
            "headers": {
                "Cache-Control": "public, max-age=36000, immutable",
                "Content-Type": "application/manifest+json"
            },
            "dest": "/*.json"
        },
        {
            "src": "/fonts/(.*)",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            },
            "dest": "/fonts/$1"
        },
        {
            "src": "/favicon/(.*)",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            },
            "dest": "/favicon/$1"
        },
        {
            "src": "/.*",
            "headers": {
                "Cache-Control": "max-age=3600, public",
                "X-Frame-Options": "DENY",
                "X-Content-Type-Options": "nosniff",
                "X-XSS-Protection": "1; mode=block",
                "Referrer-Policy": "strict-origin-when-cross-origin",
                "Strict-Transport-Security": "max-age=63072000; includeSubDomains; preload",
                "Accept-CH": "DPR, Viewport-Width, Width",
                "X-UA-Compatible": "IE=edge",
                "Content-Security-Policy": "default-src 'self'; font-src 'self' https://fonts.gstatic.com; style-src 'self' 'unsafe-inline'; img-src 'self' https://res.cloudinary.com https://okikio.dev data:; script-src 'self' https://res.cloudinary.com https://polyfill.io; connect-src 'self'; block-all-mixed-content; upgrade-insecure-requests; base-uri 'self'; object-src 'none'; worker-src 'self'; manifest-src 'self'; media-src 'self' https://res.cloudinary.com/; form-action 'self'; frame-ancestors 'self';",
                "Permissions-Policy": "geolocation=(), microphone=(), usb=(), sync-xhr=(self), camera=()",
                "Link": "</css/app.min.css>; rel=preload; as=style, </fonts/manrope-700-latin.woff2>; rel=preload; as=font; type=font/woff2; crossorigin=anonymous, </fonts/playfair-display-900-latin.woff2>; rel=preload; as=font; type=font/woff2; crossorigin=anonymous, <https://polyfill.io>; rel=preconnect; crossorigin, <https://res.cloudinary.com>; rel=preconnect; crossorigin"
            },
            "continue": true
        },
        {
            "src": "/about",
            "dest": "/about.html"
        },
        {
            "src": "/team/(.*)",
            "dest": "/team/$1.html"
        },
        {
            "src": "/service",
            "dest": "/service.html"
        },
        {
            "src": "/services/(.*)",
            "dest": "/services/$1.html"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "status": 404,
            "dest": "/404.html"
        }
    ],
    "github": {
        "silent": true
    }
}